[
    {
        "selector": "node",
        "style": {
            "background-color": "#000000",
            "label": "data(title)",
            "width": "10px",
            "height": "10px",
            "font-size": "3px",
            "text-wrap": "wrap",
            "text-max-width": "40px",
            "text-outline-width": "1px",
            "text-outline-color": "#ffffff",
            "line-height": "1.3",
            "text-margin-y": "-1px",
            "overlay-padding": "1px",
            "overlay-shape": "ellipse",
            "overlay-color": "#1f6fb8"
        }
    },
    {
        "selector": "node:selected",
        "style": {
            "background-color": "#1f6fb8"
        }
    },
    {
        "selector": "edge",
        "style": {
            "width": 1,
            "label": "data(properties.property_key)",
            "font-size": "3px",
            "text-wrap": "wrap",
            "text-max-width": "40px",
            "text-outline-width": "1px",
            "color": "#555555",
            "text-outline-color": "#ffffff",
            "line-color": "#4b5663",
            "overlay-padding": "1px",
            "overlay-color": "#1f6fb8",
            "target-arrow-color": "#4b5663",
            "target-arrow-shape": "triangle",
            "curve-style": "bezier"
        }
    },
    {
        "selector": "edge[property_key=\"guidance_references_legislation\"]",
        "style": {
            "label": "data(properties.guidance_references_legislation)"
        }
    },
    {
        "selector": "edge[property_key=\"same_named_entity\"]",
        "style": {
            "label": "data(properties.same_named_entity)"
        }
    },
    {
        "selector": "edge[stale=\"True\"]",
        "style": {
            "line-color": "#f44336",
            "target-arrow-color": "#f44336"
        }
    },
    {
        "selector": "edge[stale=\"False\"]",
        "style": {
            "line-color": "#8bc34a",
            "target-arrow-color": "#8bc34a"
        }
    }
]